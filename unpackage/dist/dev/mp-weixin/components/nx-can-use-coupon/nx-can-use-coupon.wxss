.pop-content {
  position: relative;
  background-color: #F7F7F7;
  height: 100%;
}
.pop-content .autoheight {
  min-height: 190px;
}
.pop-content .okisee {
  background: linear-gradient(132deg, #C11920 0%, #EE2626 100%);
  width: 100%;
  border-radius: 48px;
  text-align: center;
  color: #FFFFFF;
  font-size: 32rpx;
  font-weight: bold;
  line-height: 76rpx;
}

